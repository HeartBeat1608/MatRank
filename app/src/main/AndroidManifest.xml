<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.matrank">

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_dashboard_black_150dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_dashboard_black_150dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OutputActivity" />
        <activity android:name=".InputActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DeterminantActivity" />
    </application>

</manifest>